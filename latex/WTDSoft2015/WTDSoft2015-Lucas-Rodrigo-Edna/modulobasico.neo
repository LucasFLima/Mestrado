module Catalogo {
  path = "/catalogo";

  struct ItemDoCatalogo {
    string id;
    string descricao;
    float valor;         
  };
  service Catalogo {
    path = "/catalogo";
    @put Catalogo atualizarItem(string id, ItemCatalogo itemCatalogo);
    @get Catalogo pesquisarItem(string id);
  };
}